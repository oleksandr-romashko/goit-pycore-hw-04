    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with 'picture' argument",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "picture"
            ]
        },
        {
            "name": "Python Debugger: Current File with 'picture' and '--tree' arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "picture",
                "--tree"
            ]
        }
    ]
}